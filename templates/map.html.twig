{% extends 'base.html.twig' %}
{% block title %}Cloud map{% endblock %}
{% block body %}
    <div id="map"></div>
    <div id="time"{% if wallpaper %} style="display: none;"{% endif %}>
        <label for="rewind">{{ display }}</label>
        <input type="range" min="-{{ min }}" max="0" name="rewind" id="rewind" value="0" data-time="{{ time }}">
    </div>
{% endblock %}
